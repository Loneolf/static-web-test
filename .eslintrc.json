{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "react-app",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/no-import-module-exports": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "no-console": 0
        // "no-sparse-arrays": 0
    }
}
